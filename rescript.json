{
  "name": "algorithm-d-rescript",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "gentypeconfig": {
    "module": "esmodule",
    "moduleResolution": "node",
    "generatedFileExtension": ".gen.ts",
    "shims": {
      "Dict": "Dict",
    },
  },
  "suffix": ".res.mjs",
  "bs-dependencies": [
    "@rescript/core"
  ],
  "bsc-flags": [
    "-open RescriptCore"
  ]
}
