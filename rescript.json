{
  "name": "algorithm-d-rescript",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "gentypeconfig": {
    "module": "esmodule",
    "moduleResolution": "node",
    "generatedFileExtension": ".gen.ts"
  },
  "suffix": ".res.mjs",
  "bs-dependencies": [
    "@rescript/core",
    "vitest"
  ],
  "bs-dev-dependencies": [
    "vitest"
  ],
  "bsc-flags": [
    "-open RescriptCore"
  ]
}
